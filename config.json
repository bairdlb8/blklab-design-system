{
  "source": ["design-tokens/**/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "src/scss/",
      "files": [
        {
          "destination": "_design-tokens.scss",
          "format": "css/variables"
        },
        {
          "destination": "_fallback-tokens.scss",
          "format": "scss/variables"
        }
      ]
    }
  }
}
